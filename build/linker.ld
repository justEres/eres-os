ENTRY(_start)

SECTIONS
{
  . = 0x8000;

  .text : ALIGN(16) {
    *(.text.boot)
    *(.text .text.*)
  }

  .rodata : ALIGN(16) {
    *(.rodata .rodata.*)
  }

  .data : ALIGN(16) {
    *(.data .data.*)
  }

  .bss : ALIGN(16) {
    *(COMMON)
    *(.bss .bss.*)
  }

  /DISCARD/ : {
    *(.comment)
    *(.eh_frame*)
    *(.note*)
  }
}
